  echo "warranty; not even for MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE."
else
  # theargs=
  exec $thecxx -Wno-unknown-warning-option "${@/-fno-var-tracking-assignments/}"
  # exec $thecxx "$theargs"
fi
